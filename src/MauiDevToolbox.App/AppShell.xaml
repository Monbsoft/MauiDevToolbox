<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Monbsoft.MauiDevToolbox.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Monbsoft.MauiDevToolbox.App"
    xmlns:views="clr-namespace:Monbsoft.MauiDevToolbox.App.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="Dev Toolbox">

    <FlyoutItem>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem>

        <ShellContent
        Title="About"
        ContentTemplate="{DataTemplate views:AboutPage}"
        Route="AboutPage" />
    </FlyoutItem>
</Shell>